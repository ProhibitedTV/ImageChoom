# Wallpaper pack (HD)
# Run from repository root so this input path resolves correctly.
set input_config = "apps/wallpaper/inputs/themes.json"
set themes = read_json(input_config)

set api_url = "http://127.0.0.1:7860/sdapi/v1/txt2img"
set output_path = "outputs/wallpaper-pack-hd.png"
set payload = themes.hd

adapter a1111_txt2img {
  url: api_url,
  body: payload,
  output_file: output_path
}
